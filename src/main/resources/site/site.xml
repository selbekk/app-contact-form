<?xml version="1.0" encoding="UTF-8"?>
<site>
  <config>
      <input name="subject" type="TextLine">
          <label>E-mail subject</label>
          <occurrences minimum="0" maximum="1" />
          <default>Message from your website</default>
      </input>
      <input name="body" type="HtmlArea">
          <label>E-mail body</label>
          <occurrences minimum="0" maximum="1" />
          <default></default>
      </input>
  </config>
  <filters>
    <response-filter name="contact-form" order="10"/>
  </filters>
</site>
